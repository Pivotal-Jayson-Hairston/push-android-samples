<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- Default values for settings: -->

    <!-- Most of these default values were used by the developer of this library on the staging server and they      -->
    <!-- may not be available when you develop your project.  See the README.md file for more information on         -->
    <!-- setting up your own project.                                                                                -->

    <!-- You are free to set this parameter to whatever you like.  It is used during the registration process.       -->
    <string name="pref_default_device_alias">push-sample-alias</string>

    <!-- Are geofences enabled in the sample app by default?  True or false.                                         -->
    <bool name="pref_default_are_geofences_enabled">true</bool>

    <!-- This parameter is only required if you want to push messages via FCM in the sample app, though you may      -->
    <!-- also have to provide it in your "variant" in the Pivotal CF Mobile Services Push Server admin console.      -->
    <string name="pref_default_fcm_server_key"></string>

    <string name="pref_default_baidu_api_key"></string>

    <!-- These two parameters are only required if you want to send push messages via the Pivotal CF Mobile Services -->
    <!-- server in the sample app.  They are not used during the registration process.  You should be able to obtain -->
    <!-- them by creating an application in the Pivotal CF Mobile Services Push Server admin console.                -->
    <string name="pref_default_pcf_app_uuid"></string>
    <string name="pref_default_pcf_api_key"></string>
    <string name="pref_default_pcf_platform_uuid"></string>
    <string name="pref_default_pcf_platform_secret"></string>
    <string name="pref_default_pcf_server_url"></string>

</resources>
